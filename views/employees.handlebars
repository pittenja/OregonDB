  <h2>Manage Employees</h2>



  <!-- New Entry !-->
  <h3>Add Employee</h3>
<form id="employee-insert-form">
  <fieldset>
  <legend>New Employee Submit Form</legend>
  <label for="name">First Name:
  <input type="text" name="employeeFirstName" id="employeeFirstName"></label>
  <br></br>
  <label for="name">Last Name:
  <input type="text" name="employeeLastName" id="employeeLastName"></label>

  <br></br>

  <!--Submit Employee Insert-->
  <div>
    <input type="submit" id="employee-submit" class="button">
  </div>
  </fieldset>
</form>

<h3>View All Employees</h3>
<br></br>
<table>
<thead>
  <tr>
    {{#each results.[0]}}
      <th>{{@key}}</th>
    {{/each}}
  </tr>
</thead>
<tbody>
  {{#each results}}
    <tr>
    	{{#each this}}
        <td>{{this}}</td>
      {{/each}}
    </tr>
  {{/each}}
</tbody>
</table>

